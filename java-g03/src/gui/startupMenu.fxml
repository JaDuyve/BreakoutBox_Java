<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root styleClass="anchor" stylesheets="@../css/app.css" type="BorderPane" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="50.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" style="-fx-font-family: 'Roboto Light';" text="BREAKOUT BOX" textAlignment="CENTER" textFill="WHITE">
               <font>
                  <Font name="Roboto Light" size="96.0" />
               </font>
            </Label>
            <Label style="-fx-font-size: 48; -fx-font-family: 'Roboto Thin';" text="Controlepanelen" textFill="WHITE">
               <VBox.margin>
                  <Insets bottom="50.0" top="200.0" />
               </VBox.margin>
            </Label>
            <VBox alignment="CENTER" spacing="50.0">
               <children>
                  <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" spacing="50.0">
                     <children>
                        <JFXButton fx:id="btnSesstieToevoegen" alignment="CENTER" onAction="#showSessieToevoegen" prefHeight="100.0" prefWidth="300.0" text="Sessies" textAlignment="CENTER" textFill="WHITE">
                           <font>
                              <Font name="Roboto" size="24.0" />
                           </font>
                        </JFXButton>
                        <JFXButton fx:id="btnBobB1" alignment="CENTER" onAction="#showBobBeheren" prefHeight="100.0" prefWidth="300.0" text="Breakout Boxes" textAlignment="CENTER" textFill="WHITE" textOverrun="WORD_ELLIPSIS" wrapText="true">
                           <font>
                              <Font name="Roboto" size="24.0" />
                           </font>
                        </JFXButton>
                        <JFXButton fx:id="btnOefB" alignment="CENTER" onAction="#showOefBeheren" prefHeight="100.0" prefWidth="300.0" text="Oefeningen" textAlignment="CENTER" textFill="WHITE">
                           <font>
                              <Font name="Roboto" size="24.0" />
                           </font>
                        </JFXButton>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" spacing="50.0">
                     <children>
                        <JFXButton fx:id="btnDoelstellingscodeToevoegen" alignment="CENTER" onAction="#showDoelstellingscodeToevoegen" prefHeight="100.0" prefWidth="300.0" text="Doelstellingscodes" textFill="WHITE">
                           <font>
                              <Font name="Roboto" size="24.0" />
                           </font>
                        </JFXButton>
                        <JFXButton fx:id="btnVakken" alignment="CENTER" onAction="#showVakken" prefHeight="100.0" prefWidth="300.0" text="Vakken" textFill="WHITE">
                           <font>
                              <Font name="Roboto" size="24.0" />
                           </font>
                        </JFXButton>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
   </center>
</fx:root>
